{
    "name": "jelrikvh/todo",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "symfony/console": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/filesystem": "5.2.*",
        "symfony/finder": "5.2.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.2.*",
        "symfony/yaml": "5.2.*"
    },
    "require-dev": {
        "infection/infection": "^0.21.0",
        "phpmd/phpmd": "^2.9",
        "phpstan/phpstan": "^0.12.74",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Todo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Todo\\": [ "tests/" ]
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "bin-dir": "vendor/bin",
            "require": "5.2.*"
        }
    }
}
